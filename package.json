{
  "name": "huss",
  "version": "1.0.0",
  "description": "API endpoints for an ads listing platform",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test &&  sequelize db:migrate:undo:all  && sequelize db:migrate  && nyc --require @babel/register  mocha ./api/tests/*.js --timeout 20000 --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "start": "nodemon --exec babel-node ./api/index.js",
    "migrate": "sequelize db:migrate",
    "drop-db": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weylar/huss.git"
  },
  "keywords": [
    "ads",
    "buy",
    "sell"
  ],
  "author": "Usman Adio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weylar/huss/issues"
  },
  "homepage": "https://github.com/weylar/huss#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@sendgrid/mail": "^6.4.0",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
